{
  "name": "file-and-folder-tool",
  "displayName": "File And Folder Tool",
  "description": "Copy names or paths of open files, and more.",
  "version": "0.0.4",
  "publisher": "YongXianYin",
  "icon": "images/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yinyongxian/file-and-folder-tool"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "fftOthers",
        "groupId": "fftOthers",
        "label": "Others"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "command": "fft.copyFileName",
          "group": "2_copy@1"

        },
        {
          "command": "fft.copyFileNameWithoutExtension",
          "group": "2_copy@2"
        },
        {
          "command": "fft.copyDirectoryPath",
          "group": "2_copy@3"
        },
        {
          "submenu": "fftOthers",
          "group": "2_copy@4"
        }
      ],
      "fftOthers": [
        {
          "command": "fft.copyAllFileName",
          "group": "fftOthers@1"
        },
        {
          "command": "fft.copyAllFileNameWithoutExtension",
          "group": "fftOthers@2"
        },
        {
          "command": "fft.copyAllDirectoryPath",
          "group": "fftOthers@3"
        }
      ]
    },
    "commands": [
      {
        "command": "fft.copyFileName",
        "title": "Copy File Name"
      },
      {
        "command": "fft.copyFileNameWithoutExtension",
        "title": "Copy File Name Without Extension"
      },
      {
        "command": "fft.copyDirectoryPath",
        "title": "Copy Directory Path"
      },
      {
        "command": "fft.copyAllFileName",
        "title": "Copy All File Name"
      },
      {
        "command": "fft.copyAllFileNameWithoutExtension",
        "title": "Copy All File Name Without Extension"
      },
      {
        "command": "fft.copyAllDirectoryPath",
        "title": "Copy All Directory Path"
      }
    ]
  },
  "scripts": {},
  "devDependencies": {}
}
