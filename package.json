{
  "name": "file-and-folder-tool",
  "displayName": "File And Folder Tool",
  "description": "Copy file name, full path, directory path, or open the file directory.",
  "version": "0.0.1",
  "publisher": "YongXianYin",
  "icon": "images/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yinyongxian/file-and-folder-tool"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/title/context": [
        {
          "command": "fft.copyFileName",
          "group": "2_copy@1"

        },
        {
          "command": "fft.copyFileNameWithoutExtension",
          "group": "2_copy@2"
        },
        {
          "command": "fft.copyDirectoryPath",
          "group": "2_copy@3"
        }
      ]
    },
    "commands": [
      {
        "command": "fft.copyFileName",
        "title": "FFT: Copy File Name"
      },
      {
        "command": "fft.copyFileNameWithoutExtension",
        "title": "FFT: Copy File Name Without Extension"
      },
      {
        "command": "fft.copyDirectoryPath",
        "title": "FFT: Copy Directory Path"
      }
    ]
  },
  "scripts": {},
  "devDependencies": {}
}
